<view class="container">

  <image src="/images/activity/audi.jpg" style="width: 100%;display:block" mode="widthFix"></image>
  <view class="pro">
    <view class="item">
      <image src="/images/activity/audi-01.jpg" mode="widthFix"></image>
    </view>
    <view class="item">
      <image src="/images/activity/audi-02.jpg" mode="widthFix"></image>
    </view>
  </view>
  <view class="brandInfo">
    <image src="/images/activity/audi-logo.png" mode="widthFix" style="width:18%"></image>
