  </view>
  <view class="contact">
    <view class="item" bindtap="callTel" data-tel="4008306349">
      <image src="/images/activity/tel.png" mode="widthFix"></image>
      服务热线
    </view>
    <view class="item" bindtap="redirectMap">
      <image src="/images/activity/address.png" mode="widthFix"></image>
