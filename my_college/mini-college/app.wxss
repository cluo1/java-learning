@import "/wxss/weui.wxss";

page {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  font-family: 'microsoft yahei', Arial, Helvetica, sans-serif;
  font-size: 1em;
  background: #efeff4;
  line-height: 1.6;
  color: #505050
}


@media screen and (max-width: 320px) {
  page {
    font-size: 1em;
  }
}
.fs-05 {
  font-size: 0.55em !important;
}
.fs-06 {
  font-size: 0.6em !important;
}

.fs-07 {
  font-size: 0.7em !important;
}

.fs-08 {
  font-size: 0.8em !important;
}

.fs-09 {
  font-size: 0.9em !important;
}

.fs-10 {
  font-size: 1em !important;
}

.fs-11 {
  font-size: 1.1em !important;
}

.fs-12 {
  font-size: 1.2em !important;
}
.fs-13 {
  font-size: 1.3em !important;
}

.fs-15 {
  font-size: 1.5em !important;
}

.fs-17 {
  font-size: 1.7em !important;
}

.fs-20 {
  font-size: 2.0em !important;
}

.fs-30 {
  font-size: 3.0em !important;
}

/*me*/

.pagespacing {
  margin-bottom: 5em;
}




/*.placeholder {
  margin: 5px;
  text-align: center;
  background: #ebebeb;
  color: #cfcfcf;
}*/


.hide {
  display: none !important;
}

.fix-bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  text-align: center;
  background: #fff;
  -webkit-box-shadow: 0 0 10px #e0dede;
  -moz-box-shadow: 0 0 10px #e0dede;
  box-shadow: 0 0 10px #e0dede;
  z-index: 999;
}

.fix-bottom .submit {
  display: inline-block;
  width: 100%;
  height: 100%;
  border-radius: 0 !important;
  background: #48a7dd !important;
  color: #fff;
  line-height: 3em;
}








.fixbtn {
  position: fixed;
  bottom: 0;
  width: 100%;
  text-align: center;
  background: #fff;
  -webkit-box-shadow: 0 0 10px #e0dede;
  -moz-box-shadow: 0 0 10px #e0dede;
  box-shadow: 0 0 10px #e0dede;
}

.fixbtn navigator {
  display: inline;
}

.fixbtn button {
  display: inline-block;
  width: 45%;
  margin: 5px;
  border: 1px solid #48a7dd;
  border-radius: 0 !important;
}

.fixbtn button:after {
  border-radius: 0 !important;
}

.fixbtn .addbtn {
  background: #48a7dd;
  color: #fff;
}

.fixbtn .buildbtn {
  background: #fff;
  color: #48a7dd;
}



.noicon {
  display: block;
  width: 200px;
  height: 200px;
  text-align: center;
  margin: 0 auto;
  padding: 20px 0;
}

.pholder {
  color: #b2b2b2;
}

.btn {
  width: 90%;
  margin: 0 5%;
  background: #48a7dd;
  color: #fff;
  border: 1px solid #48a7dd;
  margin-bottom: 15px;
  border-radius: 5px;
}

.bascibtn {
  width: 90%;
  margin: 0 5%;
  background: #f8f8f8;
  color: #000;
  border: 1px solid #e5e5e5;
  margin-bottom: 15px;
  border-radius: 5px;
}

.gray {
  background: #888 !important;
  color: #fff !important;
}
