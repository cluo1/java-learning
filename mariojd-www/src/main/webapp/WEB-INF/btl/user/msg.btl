<body style="background-color: #eeeeee">
<div class="container text-center text-danger" role="main">
    <div class="jumbotron">
        <br>
        <h2>
            <b> 系统提示 </b>
        </h2>

        <p>
            <small><b>${messageResult.message}，<span id="msg">8</span>秒后将转到网站主页</b></small>
        </p>
    </div>
</div>
<script type="text/javascript">
    var msg = document.getElementById("msg");
    window.onload = function () {
        var time = 8;
        var timer = setInterval(function () {
            msg.innerHTML = time;
            time--;
            if (time < 0) {
                window.location.href = '/';
                clearInterval(timer);
            }
        }, 1000);
    };
</script>
