<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.CourseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.Course">
        <id column="id" property="id" />
        <result column="cid" property="cid" />
        <result column="tid" property="tid" />
        <result column="uid" property="uid" />
        <result column="num" property="num" />
        <result column="name" property="name" />
        <result column="start_date" property="startDate" />
        <result column="end_date" property="endDate" />
        <result column="week_day" property="weekDay" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="status" property="status" />
        <result column="createtime" property="createtime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, cid, tid, uid, num, name, start_date AS startDate, end_date AS endDate, week_day AS weekDay, start_time AS startTime, end_time AS endTime, status, createtime
    </sql>

</mapper>
